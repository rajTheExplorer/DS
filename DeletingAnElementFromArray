class DeletingAnElementFromArray
{
	public static void main(String[] args) {
		// Deleting an element from the array which is at position 3
		int position=3;
		int[] arr={100,99,88,77,66};
		for(int i=position-1;i<arr.length-1;i++)
		{
			arr[i]=arr[i+1];
		}
		arr[arr.length-1]=0;
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]+" ");
		}

	}
}
